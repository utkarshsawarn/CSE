        "#7\n",
        "def is_harshad(n):\n",
        "    if not isinstance(n, int) or n <= 0:\n",
        "        raise ValueError(\"Input must be a positive integer.\")\n",
        "\n",
        "    sum_of_digits = 0\n",
        "    temp_n = n\n",
        "    while temp_n > 0:\n",
        "        sum_of_digits += temp_n % 10\n",
        "        temp_n //= 10\n",
        "\n",
        "    return n % sum_of_digits == 0\n",
        "\n",
        "print(is_harshad(4563897))\n",
        "print(is_harshad(7698707))\n",
        "print(is_harshad(5658))\n"
      ],
